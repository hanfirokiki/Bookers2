<h1>User info</h1>
  <%= form_with model: @book do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    
    <h1>New Book</h1>
    
    <h4>Titile</h4>
      <%= f.text_field :book_title %>
    <h4>Opinion</h4>
      <%= f.text_area :opinion %>
    <%= f.submit '投稿' %>
  <% end %>